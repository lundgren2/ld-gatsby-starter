{
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": 0,
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-boolean-value": 0,
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0,
  },
  "plugins": ["react-hooks"]
}
